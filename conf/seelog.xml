<seelog type="asynctimer" asyncinterval="5000000" minlevel="debug" maxlevel="error">
   
	
    <outputs formatid="main">
        <console/>
         <file path="logs/log.log" />

        <filter levels="error">
            <file path="./logs/error.log"/>
        </filter> 

    </outputs>
    <formats>
        <format id="main" format="%Date %Time [%Level] %Msg%n"/>
        
    </formats>
</seelog>